<<DefineFunc,include=FALSE>>=
Tquantil <- function( x , probs )
    round(
        quantile( x = x , na.rm = TRUE , probs = probs ) ,
        digits = 1)

describe <- function(X){
    DTAB <- c(
        as.character(sum(!is.na(X))) ,
        Tquantil( X , 0.250 ) ,
        Tquantil( X , 0.500 ) ,
        Tquantil( X , 0.750 ))
        attr( DTAB , "names") <- c(
            "N" , "25%", "50%", "75%")
        return(DTAB)}
@ 





